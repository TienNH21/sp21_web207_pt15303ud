\n      onUpdateProduct();\n    }\n  }\n\n  const btnClearOnClick = () => {\n    setClicked(-1);\n    setFormData({\n      id: '',\n      name: '',\n      price: '',\n    });\n  }\n\n  return (\n    <form\n      onSubmit={ onSubmitHandler }\n      style={{ marginTop: '10px' }}>\n      <TextField\n        label=\"Id\"\n        fullWidth\n        value={ formData.id }\n        name=\"id\"\n        onChange={ onChangeHandler }\n        style={{ marginTop: '10px' }}\n        variant=\"outlined\" />\n      <TextField\n        label=\"Name\"\n        fullWidth\n        value={ formData.name }\n        name=\"name\"\n        onChange={ onChangeHandler }\n        style={{ marginTop: '10px' }}\n        variant=\"outlined\" />\n      <TextField\n        label=\"Price\"\n        fullWidth\n        value={ formData.price }\n        name=\"price\"\n        onChange={ onChangeHandler }\n        style={{ marginTop: '10px' }}\n        variant=\"outlined\" />\n      <Button\n        type=\"submit\"\n        variant=\"contained\"\n        color=\"primary\">\n        Submit\n      </Button>\n      <Button\n        onClick={ btnClearOnClick }\n        variant=\"contained\"\n        color=\"default\">\n          Clear\n      </Button>\n    </form>\n  );\n}\n\nexport default CreateProduct;\n",["40","41"],"/Users/tiennh/Desktop/Sp21_WEB207/pt15303ud/src/reportWebVitals.js",[],["42","43"],"/Users/tiennh/Desktop/Sp21_WEB207/pt15303ud/src/index.js",[],"/Users/tiennh/Desktop/Sp21_WEB207/pt15303ud/src/components/ListProducts.js",["44","45"],"import React from 'react';\nimport Table from '@material-ui/core/Table';\nimport TableBody from '@material-ui/core/TableBody';\nimport TableCell from '@material-ui/core/TableCell';\nimport TableHead from '@material-ui/core/TableHead';\nimport TableRow from '@material-ui/core/TableRow';\nimport Button from '@material-ui/core/Button';\nimport { useEffect } from 'react';\nimport axios from 'axios';\n\nfunction ListProducts({\n  data,\n  setClicked,\n  setFormData,\n  setProducts,\n}) {\n  const onClickHandler = (event, value, index) => {\n    setClicked(index);\n    setFormData(value);\n  }\n\n  const onDelete = (value, index) => {\n    const url = 'https://5f2d045b8085690016922b50.mockapi.io/todo-list/products/' + value.id;\n\n    return axios({\n      method: 'delete',\n      url: url,\n    })\n  }\n\n  const btnDeleteOnClick = (event, value, index) => {\n    const result = onDelete(value, index);\n\n    result.then((response) => {\n        setProducts((oldState) => {\n          let newState = oldState.filter((value, idx) => {\n            return idx == index ? false : true;\n          });\n\n          return newState;\n        });\n      })\n      .catch((error) => {\n        console.log(error);\n      })\n  }\n\n  return (\n    <div>\n      <Table>\n        <TableHead>\n          <TableRow>\n            <TableCell>Id</TableCell>\n            <TableCell>Name</TableCell>\n            <TableCell>Price</TableCell>\n            <TableCell>Action</TableCell>\n          </TableRow>\n        </TableHead>\n        <TableBody>\n          {\n            data.map((value, index) => {\n              return (\n                <TableRow\n                  onClick={\n                    (event) => {\n                      onClickHandler(event, value, index);\n                    }\n                  }\n                  key={index}>\n                  <TableCell>{ value.id }</TableCell>\n                  <TableCell>{ value.name }</TableCell>\n                  <TableCell>{ value.price }</TableCell>\n                  <TableCell>\n                    <Button\n                      onClick={ (event) => btnDeleteOnClick(event, value, index) }\n                      color=\"secondary\">Delete</Button>\n                  </TableCell>\n                </TableRow>\n              );\n            })\n          }\n        </TableBody>\n      </Table>\n    </div>\n  );\n}\n\nexport default ListProducts;\n","/Users/tiennh/Desktop/Sp21_WEB207/p